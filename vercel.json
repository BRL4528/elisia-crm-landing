{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist/public",
  "headers": [
    {
      "source": "/og-image.png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/privacy-policy",
      "destination": "/privacy-policy.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
